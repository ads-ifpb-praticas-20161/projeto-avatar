<?xml version="1.0" encoding="UTF-8"?>
<Confguration>
    <Appenders>
        <Console  name="Console"  target ="SYSTEM_OUT">
        <PatternLayout  
            pattern = "%d{HH:mm:ss:SS} [%t] [%level %logger{3} = %msg%n"/>       
        </Console>
    </Appenders>
    <Loggers>
        <Root level="error">
            <AppenderRef ref = "Console"/>
        </Root>
    </Loggers>
</Confguration>
